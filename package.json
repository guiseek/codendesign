{
  "name": "codendesign",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "nx": "node --max_old_space_size=8000 ./node_modules/.bin/nx",
    "start": "ng serve",
    "build": "nx run demo:build --prod --output-hashing none && scully --pr demo --sr --prod",
    "clean": "rimraf ./dist",
    "build:ci": "npm run clean && npm run affected:test && npm run storybook:ci",
    "storybook:ci": "npm run nx run code-components:build-storybook --configuration=ci",
    "semantic:ci": "npm run semantic-release --debug --ci --dry-run",
    "test:ci": "",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "commit": "git-cz",
    "storybook": "ng run code-components:storybook",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:build:ci": "nx affected:build --base=remotes/origin/master",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:test:ci": "nx affected:test --base=remotes/origin/master",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "cmt": "git-cz",
    "local-registry": "./scripts/local-registry.sh",
    "feat:start": "git flow feature start",
    "feat:finish": "git flow feature finish",
    "semantic-release": "semantic-release",
    "playground": "angular-playground",
    "scully": "scully",
    "scully:serve": "scully serve",
    "scully:watch": "scully -- --watch",
    "affected:deploy": "ts-node --project tools/tsconfig.tools.json tools/tasks/deploy.ts",
    "pocs-elements:single-build": "ts-node --project tools/tsconfig.tools.json tools/tasks/deploy.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.0",
    "@angular/cdk": "9.2.4",
    "@angular/common": "^9.1.0",
    "@angular/compiler": "^9.1.0",
    "@angular/core": "^9.1.0",
    "@angular/fire": "^6.0.2",
    "@angular/forms": "^9.1.0",
    "@angular/material": "9.2.4",
    "@angular/platform-browser": "^9.1.0",
    "@angular/platform-browser-dynamic": "^9.1.0",
    "@angular/router": "^9.1.0",
    "@nrwl/angular": "9.5.1",
    "@nrwl/nx-plugin": "^9.5.1",
    "@scullyio/init": "0.0.29",
    "@scullyio/ng-lib": "0.0.27",
    "@scullyio/scully": "0.0.102",
    "@types/highlight.js": "^9.12.4",
    "@types/listr": "^0.14.2",
    "cz-customizable": "^6.2.1",
    "firebase": "^7.13.1",
    "listr": "^0.14.3",
    "lodash": "^4.17.19",
    "posthtml": "^0.13.1",
    "posthtml-highlight": "^1.1.1",
    "prismjs": "^1.20.0",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/architect": ">= 0.900 < 0.1100",
    "@angular-devkit/build-angular": "0.901.0",
    "@angular-devkit/build-ng-packagr": "~0.901.11",
    "@angular/cli": "9.1.0",
    "@angular/compiler-cli": "^9.1.0",
    "@angular/language-service": "^9.1.0",
    "@ngneat/spectator": "^5.11.0",
    "@nrwl/cypress": "9.5.1",
    "@nrwl/jest": "9.5.1",
    "@nrwl/nx-cloud": "^9.3.5",
    "@nrwl/storybook": "^9.5.1",
    "@nrwl/workspace": "9.5.1",
    "@semantic-release/github": "^7.0.7",
    "@storybook/addon-a11y": "^5.3.19",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-docs": "^5.3.19",
    "@storybook/addon-jest": "^5.3.19",
    "@storybook/addon-knobs": "5.3.9",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addon-notes": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/angular": "5.3.9",
    "@storybook/theming": "^5.3.19",
    "@types/execa": "^2.0.0",
    "@types/jest": "25.1.4",
    "@types/lodash": "^4.14.157",
    "@types/node": "~8.9.4",
    "@types/ramda": "^0.27.14",
    "angular-playground": "^7.0.3",
    "codelyzer": "~5.0.1",
    "concat": "^1.0.3",
    "cypress": "^4.1.0",
    "cz-conventional-changelog": "^3.2.0",
    "dotenv": "6.2.0",
    "eslint": "6.8.0",
    "execa": "^4.0.3",
    "firebase-tools": "^8.0.0",
    "fuzzy": "^0.1.3",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jest": "25.2.3",
    "jest-preset-angular": "8.1.2",
    "ng-packagr": "^9.0.0",
    "open": "^7.0.3",
    "prettier": "2.0.4",
    "ramda": "^0.27.1",
    "semantic-release": "^17.1.1",
    "ts-jest": "25.2.1",
    "ts-node": "~7.0.0",
    "tslint": "~6.0.0",
    "typescript": "~3.8.3"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.js"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  }
}
